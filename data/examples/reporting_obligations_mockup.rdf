    <dgfro:hasReportingObligation>
      <dgfro:ReportingObligation rdf:nodeID="Ncfa10d35e7ad43e889d05e7c3f302343">
        <rdf:value>In case the ECB issues additional invoices or recalculates the annual supervisory fee in accordance with Article 7 of Regulation ( EU ) No 1163/2014 , the ECB shall communicate to the Board the new data within 5 days of the issuance of fee notices .</rdf:value>
        <dgfro:hasVerb>
          <dgfro:Verb rdf:nodeID="N2eb40c9002aa45fca043d5fe2d8ed918">
            <skos:prefLabel xml:lang="en">communicate</skos:prefLabel>
          </dgfro:Verb>
        </dgfro:hasVerb>
        <dgfro:hasPropTmp>
          <dgfro:PropTmp rdf:nodeID="N96da65d05faf4bc494e88e57675fc976">
            <skos:prefLabel xml:lang="en">within 5 days of the issuance of fee notices</skos:prefLabel>
          </dgfro:PropTmp>
        </dgfro:hasPropTmp>
        <dgfro:hasPropAdv>
          <dgfro:PropAdv rdf:nodeID="N5ffe029440434ff3874d33084f74430f">
            <skos:prefLabel xml:lang="en">In case the ECB issues additional invoices or recalculates the annual supervisory fee in accordance with Article 7 of Regulation ( EU ) No 1163/2014</skos:prefLabel>
          </dgfro:PropAdv>
        </dgfro:hasPropAdv>
        <dgfro:hasRegulatoryBody>
          <dgfro:RegulatoryBody rdf:nodeID="N5a4758358e6144bea0657c2db0f0b4c8">
