        <dgfro:hasReport>
          <dgfro:Report rdf:nodeID="N41e9144403bb4beca2538ea6b0e359c0">
            <skos:prefLabel xml:lang="en">with the data as specified in Article 6 and collected by the ECB in accordance with Regulation ( EU ) No 1163/2014 in financial years belonging to the provisional period</skos:prefLabel>
          </dgfro:Report>
        </dgfro:hasReport>
        <dgfro:hasPropTmp>
          <dgfro:PropTmp rdf:nodeID="Naf8851476e414703a2826c19928e2dfa">
            <skos:prefLabel xml:lang="en">Within 30 days of the entry into force of this Regulation</skos:prefLabel>
          </dgfro:PropTmp>
        </dgfro:hasPropTmp>
        <dgfro:hasPropMod>
          <dgfro:PropMod rdf:nodeID="N3ce654579f5643d7b906cc97c2b371a1">
            <skos:prefLabel xml:lang="en">shall</skos:prefLabel>
