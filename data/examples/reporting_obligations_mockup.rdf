    <dgfro:hasReportingObligation>
      <dgfro:ReportingObligation rdf:nodeID="Nd0ac177cc5864c099d9680ce364ce412">
        <dgfro:hasRegulatoryBody>
          <dgfro:RegulatoryBody rdf:nodeID="Nf7a632f8d0094169b7c6005dcac302af">
