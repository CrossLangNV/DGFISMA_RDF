          <dgfro:Report rdf:nodeID="N8af1713e0c7f419e8d192e9555de7929">
            <skos:prefLabel xml:lang="en">the new data</skos:prefLabel>
          </dgfro:Report>
        </dgfro:hasReport>
        <dgfro:hasPropMod>
          <dgfro:PropMod rdf:nodeID="N9c9716cf0d8e4fdf95d45730b71017cb">
            <skos:prefLabel xml:lang="en">shall</skos:prefLabel>
          </dgfro:PropMod>
        </dgfro:hasPropMod>
        <dgfro:hasReporter>
          <dgfro:Reporter rdf:nodeID="Nb6f9a3a570a943da9b2b2dfe9dce2925">
            <skos:prefLabel xml:lang="en">the ECB</skos:prefLabel>
